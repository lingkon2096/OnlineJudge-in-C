#include<stdio.h>
int main(){
   int sum,t,a,b,i,j;
scanf("%d", &t);
for(j=1;j<=t;j++)
{
   sum=0;
   scanf("%d%d",&a,&b);
if(a>b)
{
   for(i=b;i<=a;i++)
   {
      if(i%2==1)
      {
         sum=sum+i;
      }
   }
}
else
{
for(i=a;i<=b;i++)
   {
      if(i%2==1)
      {
         sum=sum+i;
      }
   }
}
printf("Case %d : %d \n ", j, sum);
}
return 0;
}
